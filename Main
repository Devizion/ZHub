local Done1=Done1
local Done2=Done2
local Done3=Done3
local Done4=Done4
local Done5=Done5
local BasicMenu=BasicMenu
local AdvencedMenu=AdvencedMenu
local Modal=Modal

local X,Y,Z=X,Y,Z

local Player=Player
local Number=Number
local Main2=Main2

--Func
local Thread=nil
local ThreadAim=nil
local ThreadWH=nil
local Aimbot=false
local State=true
local ESPstate=true

local function tpPos(X,Y,Z)
	if typeof(X or Y or Z)~="number" then warn("не число") return end
	Exploiter.Character.PrimaryPart.CFrame=Exploiter.Character.PrimaryPart.CFrame*CFrame.new(X,Y,Z)
end

local function tpPlayer(PlayerName)
	if not Players:FindFirstChild(PlayerName) then warn("не нашёл игрока") return end
	local NowPlayer=Players:FindFirstChild(PlayerName)

	local Chr=NowPlayer.Character or NowPlayer.CharacterAdded:Wait()

	if Chr and Chr.PrimaryPart then
		Exploiter.Character.PrimaryPart.CFrame=Chr.PrimaryPart.CFrame*CFrame.new(0,0,2)
	end
end

local function setWalkSpeed(Number)
	if typeof(Number)~="number" then warn("не число") return end
	if Thread then coroutine.close(Thread) end
	Thread=coroutine.create(function()
		while task.wait() do
			local Hum=nil
			if not Exploiter.Character:FindFirstChildOfClass("Humanoid") then continue end
			Hum=Exploiter.Character:FindFirstChildOfClass("Humanoid")

			Hum.WalkSpeed=Number
		end
	end)
	coroutine.resume(Thread)
end

local function drawESP()
	for _,v in Players:GetPlayers() do
		local Model=v.Character
		if not Model or Model:FindFirstChild("ESP") or Model==Exploiter.Character then continue end

		local Bild=Instance.new("BillboardGui",Model)
		local Frame=Instance.new("Frame",Bild)
		local UIs=Instance.new("UIStroke",Frame)

		Bild.Name="ESP"
		Bild.Size=UDim2.new(4,0,6,0)
		Bild.Adornee=Model
		Bild.AlwaysOnTop=true

		Frame.BackgroundTransparency=1
		Frame.AnchorPoint=Vector2.new(0.5,0.5)
		Frame.Size=UDim2.new(1,0,1,0)
		Frame.Position=UDim2.new(0.5,0,0.5,0)

		UIs.Color=Color3.fromRGB(0, 255, 0)
		UIs.Thickness=3
	end
end

local function destoyESP()
	for _,v in Players:GetPlayers() do
		local Model=v.Character
		if not Model or not Model:FindFirstChild("ESP") then continue end

		Model.ESP:Destroy()
	end
end
--Code

local Open=TweenService:Create(MainFrame,TweenInfo.new(.5,Enum.EasingStyle.Exponential),{Position=UDim2.new(0.5, 0,0.5, 0)})
local Close=TweenService:Create(MainFrame,TweenInfo.new(.5,Enum.EasingStyle.Sine),{Position=UDim2.new(0.5, 0,-1, 0)})

UserInputService.InputBegan:Connect(function(input,gpe)
	local int=input.KeyCode
	if int==Enum.KeyCode.Insert and not gpe then
		Modal.Modal=not Modal.Modal
		if State then Open:Play() Main2.Visible=true else Close:Play() Main2.Visible=false end
		State=not State
	elseif int==Enum.KeyCode.Delete and not gpe then
		Main:Destroy()
		script:Destroy()
	end
end)
local Instructions={
	["Done1"]=function()
		tpPos(tonumber(X.Text),tonumber(Y.Text),tonumber(Z.Text))
	end,
	["Done2"]=function()
		tpPlayer(Player.Text)
	end,
	["Done3"]=function()
		setWalkSpeed(tonumber(Number.Text))
	end,
	["Done4"]=function()
		local C=nil
		local tab={}

		Done4.Text="Вкл"
		Done4.TextColor3=Color3.fromRGB(68, 211, 46)

		if ThreadAim then
			coroutine.close(ThreadAim)
			ThreadAim=nil
			return
		end

		for _,Plr in Players:GetPlayers() do
			if Plr.Character==Exploiter.Character then continue end
			C=Plr.Character
			if not C or not C.PrimaryPart then continue end
			table.insert(tab,{C,(C.PrimaryPart.Position-Exploiter.Character.PrimaryPart.Position).Magnitude})
		end
		local min=math.huge
		for _,v in tab do
			if v[2]<min then min=v[2] end
		end
		for _,v in tab do
			if v[2]==min then
				C=v[1]
				break
			end
		end

		if not C or not C.PrimaryPart then return end
		if not C:FindFirstChild("Head") then
			if C.PrimaryPart then
				ThreadAim=coroutine.create(function()
					print(C.PrimaryPart,"osn")
					while task.wait() do
						Camera.CFrame=CFrame.new(Camera.CFrame.Position,C.PrimaryPart.Position)
					end
				end)
				coroutine.resume(ThreadAim)
				Done4.Text="Выкл"
				Done4.TextColor3=Color3.fromRGB(190, 18, 18)
			else
				return
			end
		else
			ThreadAim=coroutine.create(function()
				print(C.PrimaryPart,"Head")
				while task.wait() do
					Camera.CFrame=CFrame.new(Camera.CFrame.Position,C.Head.Position)
				end
			end)
			coroutine.resume(ThreadAim)
			Done4.Text="Выкл"
			Done4.TextColor3=Color3.fromRGB(190, 18, 18)
		end
	end,
	["Done5"]=function()
		if ESPstate then Done5.Text="Выкл" Done5.TextColor3=Color3.fromRGB(190, 18, 18) drawESP() else Done5.Text="Вкл" Done5.TextColor3=Color3.fromRGB(68, 211, 46) destoyESP() end
		ESPstate=not ESPstate
	end,
}

local Dones={Done1,Done2,Done3,Done4,Done5}
for _,Button in Dones do
	Button.MouseButton1Click:Connect(function()
		Instructions[Button.Name]()
	end)
end

local MenuNow="BasicMenu"
local MenuLast=nil

local MenusB={BasicMenu,AdvencedMenu}
for _,Button in MenusB do
	Button.MouseButton1Click:Connect(function()
		MenuLast=MenuNow
		MenuNow=Button.Name
		for _,v in MainFrame.Menus[MenuNow]:GetChildren() do
			v.Visible=true
		end
		for _,v in MainFrame.Menus[MenuLast]:GetChildren() do
			if MenuLast~=MenuNow then
				v.Visible=false
			end
		end
	end)
end

UserInputService.InputBegan:Connect(function(input,gpe)
	if input.KeyCode==Enum.KeyCode[Done4.Parent.Bind.Text] and not gpe then
		Instructions[Done4.Name]()
	end
end)

local Mouse=coroutine.create(function()
	while task.wait() do
		Main2.Position=UDim2.new(0,Exploiter:GetMouse().X,0,Exploiter:GetMouse().Y)
	end
end)
coroutine.resume(Mouse)
