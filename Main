local MenuNow="BasicMenu"
local MenuLast=nil

local MenusB={BasicMenu,AdvencedMenu,NewMenu}
for _,Button in MenusB do
	Button.MouseButton1Click:Connect(function()
		MenuLast=MenuNow
		MenuNow=Button.Name
		for _,v in MainFrame.Menus[MenuNow]:GetChildren() do
			v.Visible=true
		end
		for _,v in MainFrame.Menus[MenuLast]:GetChildren() do
			if MenuLast~=MenuNow then
				v.Visible=false
			end
		end
	end)
end

Bind.MouseButton1Click:Connect(function()
	Bind.Text="Запись..."
	Bind.TextColor3=Color3.fromRGB(178, 178, 178)

	local conn
	conn=UserInputService.InputBegan:Connect(function(input)
		RegButton=input.KeyCode
		if RegButton==Enum.KeyCode.Escape then
			RegButton=Enum.KeyCode.X
		end

		Bind.Text=RegButton.Name
		Bind.TextColor3=Color3.fromRGB(255, 255, 255)
		conn:Disconnect()
	end)
end)

